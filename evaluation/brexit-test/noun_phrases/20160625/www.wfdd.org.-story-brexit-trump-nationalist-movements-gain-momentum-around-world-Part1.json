\"entities\": [], \"noun_phrases\": [{\"ngram\": \"recent decades\", \"from\": 1023, \"to\": 1037}, {\"ngram\": \"nationalist movements\", \"from\": 1039, \"to\": 1060}, {\"ngram\": \"vocal minorities\", \"from\": 1079, \"to\": 1095}, {\"ngram\": \"powerful parties\", \"from\": 1099, \"to\": 1115}, {\"ngram\": \"control\", \"from\": 1128, \"to\": 1135}, {\"ngram\": \"governments\", \"from\": 1139, \"to\": 1150}, {\"ngram\": \"places\", \"from\": 1154, \"to\": 1160}, {\"ngram\": \"Hungary\", \"from\": 1166, \"to\": 1173}, {\"ngram\": \"national elections\", \"from\": 1185, \"to\": 1203}, {\"ngram\": \"margins\", \"from\": 1223, \"to\": 1230}, {\"ngram\": \"countries\", \"from\": 1234, \"to\": 1243}, {\"ngram\": \"Austria\", \"from\": 1249, \"to\": 1256}, {\"ngram\": \"the United Kingdom\", \"from\": 1281, \"to\": 1299}, {\"ngram\": \"the European Union\", \"from\": 1307, \"to\": 1325}]}, {\"sentence\": \"Mounk pegs economic stagnation among lower- and middle-class whites as a main driver for nationalism's rise around the globe.\", \"from\": 1328, \"to\": 1453, \"entities\": [], \"noun_phrases\": [{\"ngram\": \"Mounk\", \"from\": 1328, \"to\": 1333}, {\"ngram\": \"economic stagnation\", \"from\": 1339, \"to\": 1358}, {\"ngram\": \"lower- and middle-class whites\", \"from\": 1365, \"to\": 1395}, {\"ngram\": \"a main driver\", \"from\": 1399, \"to\": 1412}, {\"ngram\": \"nationalism's rise\", \"from\": 1417, \"to\": 1435}, {\"ngram\": \"the globe\", \"from\": 1443, \"to\": 1452}]}, {\"sentence\": \"\\\"You have a socially descending middle class that has not had real gains in the standard of living in 30 years,\\\" he said.\", \"from\": 1454, \"to\": 1575, \"entities\": [], \"noun_phrases\": [{\"ngram\": \"a socially descending middle class\", \"from\": 1464, \"to\": 1498}, {\"ngram\": \"real gains\", \"from\": 1516, \"to\": 1526}, {\"ngram\": \"the standard\", \"from\": 1530, \"to\": 1542}, {\"ngram\": \"30 years\", \"from\": 1556, \"to\": 1564}]}, {\"sentence\": \"\\\