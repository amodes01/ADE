 [{\"ngram\": \"a \\\"Brexit\", \"from\": 337, \"to\": 346}, {\"ngram\": \"the resulting market turmoil\", \"from\": 352, \"to\": 380}, {\"ngram\": \"the world's safe haven\", \"from\": 421, \"to\": 443}, {\"ngram\": \"the core economies\", \"from\": 445, \"to\": 463}, {\"ngram\": \"the world\", \"from\": 477, \"to\": 486}, {\"ngram\": \"the Great Recession\", \"from\": 493, \"to\": 512}]}, {\"sentence\": \"\\\"Investors should accept the facts and reduce their remaining exposure to developed markets,\\\" Dehn said.\", \"from\": 522, \"to\": 626, \"entities\": [], \"noun_phrases\": [{\"ngram\": \"Investors\", \"from\": 523, \"to\": 532}, {\"ngram\": \"the facts\", \"from\": 547, \"to\": 556}, {\"ngram\": \"their remaining exposure\", \"from\": 568, \"to\": 592}, {\"ngram\": \"developed markets\", \"from\": 596, \"to\": 613}, {\"ngram\": \"Dehn\", \"from\": 616, \"to\": 620}]}, {\"sentence\": \"As if to underline the point, the International Monetary Fund cut its forecast for U.S. growth recently to 2.2% this year, down from 2.4% in its April 2016 World Economic Outlook.\", \"from\": 628, \"to\": 808, \"entities\": [], \"noun_phrases\": [{\"ngram\": \"the point\", \"from\": 647, \"to\": 656}, {\"ngram\": \"the International Monetary Fund\", \"from\": 658, \"to\": 689}, {\"ngram\": \"its forecast\", \"from\": 694, \"to\": 706}, {\"ngram\": \"U.S. growth\", \"from\": 711, \"to\": 722}, {\"ngram\": \"2.2%\", \"from\": 735, \"to\": 739}, {\"ngram\": \"2.4%\", \"from\": 761, \"to\": 765}, {\"ngram\": \"its April 2016 World Economic Outlook\", \"from\": 769, \"to\": 806}]}, {\"sentence\": \"The IMF cited headwinds including the strong dollar and weak corporate investment.\", \"from\": 809, \"to\": 891, \"entities\": [], \"noun_phrases\": [{\"ngram\": \"The IMF\", \"from\": 809, \"to\": 816}, {\"ngram\": \"headwinds\", \"from\": 823, \"to\": 832}, {\"ngram\": \"the strong dollar\", \"from\": 843, \"to\": 860}, {\"ngram\": \"weak corporate investment\", \"from\": 865, \"to\"