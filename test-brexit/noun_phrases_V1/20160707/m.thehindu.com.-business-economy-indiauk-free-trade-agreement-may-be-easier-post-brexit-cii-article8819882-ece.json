ities\": [], \"noun_phrases\": [{\"ngram\": \"tighter bilateral trade agreement\", \"from\": 1796, \"to\": 1829}, {\"ngram\": \"the UK\", \"from\": 1838, \"to\": 1844}, {\"ngram\": \"India\", \"from\": 1849, \"to\": 1854}, {\"ngram\": \"rapid progress\", \"from\": 1864, \"to\": 1878}]}, {\"sentence\": \"\\\"I want to reassure you about any uncertainty as the UK enters a new phase of its relationship with the EU... more than ever we are going to be an outward looking, adventurous, optimistic country,\\\" Johnson said.\", \"from\": 1891, \"to\": 2102, \"entities\": [], \"noun_phrases\": [{\"ngram\": \"any uncertainty\", \"from\": 1921, \"to\": 1936}, {\"ngram\": \"the UK\", \"from\": 1940, \"to\": 1946}, {\"ngram\": \"a new phase\", \"from\": 1954, \"to\": 1965}, {\"ngram\": \"its relationship\", \"from\": 1969, \"to\": 1985}, {\"ngram\": \"the EU\", \"from\": 1991, \"to\": 1997}, {\"ngram\": \"an outward looking, adventurous, optimistic country\", \"from\": 2035, \"to\": 2086}, {\"ngram\": \"Johnson\", \"from\": 2089, \"to\": 2096}]}, {\"sentence\": \"Indian High Commissioner to the UK Navtej Sarna said the political underpinning for the economic relationship between India and the UK was strong and that uncertainties should not have any adverse effect as business \\\"thrives of uncertainties\\\" and a \\\"planned roadmap\\\" for the relationship already exists.\", \"from\": 2104, \"to\": 2407, \"entities\": [], \"noun_phrases\": [{\"ngram\": \"Indian High Commissioner\", \"from\": 2104, \"to\": 2128}, {\"ngram\": \"the UK Navtej Sarna\", \"from\": 2132, \"to\": 2151}, {\"ngram\": \"the political underpinning\", \"from\": 2157, \"to\": 2183}, {\"ngram\": \"the economic relationship\", \"from\": 2188, \"to\": 2213}, {\"ngram\": \"India\", \"from\": 2222, \"to\": 2227}, {\"ngram\": \"the UK\", \"from\": 2232, \"to\": 2238}, {\"ngram\": \"uncertainties\", \"from\": 2259, \"to\": 2272}, {\"ngram\": \"any adverse effect\", \"from\": 2289, \"to\": 2307}, {\"ngram\": \"business\", \"from\": 2311, \"to\": 2319}, {\"ngram\": \"uncertainties\", \"from\": 2332, \"to\": 2345}, {\"ngram\": \"a \\\"planned roadmap\", \"from\": 2351, \"to\": 2369}, {\"ngram\": \"the relationship\", \"from\": 2375, \"to\": 2391}]}]}"