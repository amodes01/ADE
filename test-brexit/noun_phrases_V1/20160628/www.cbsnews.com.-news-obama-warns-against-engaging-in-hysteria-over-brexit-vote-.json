, \"noun_phrases\": [{\"ngram\": \"The president\", \"from\": 774, \"to\": 787}, {\"ngram\": \"the decision\", \"from\": 802, \"to\": 814}, {\"ngram\": \"people\", \"from\": 853, \"to\": 859}, {\"ngram\": \"the U.S.\", \"from\": 865, \"to\": 873}, {\"ngram\": \"close relationships\", \"from\": 901, \"to\": 920}, {\"ngram\": \"Great Britain\", \"from\": 926, \"to\": 939}, {\"ngram\": \"the European Union\", \"from\": 944, \"to\": 962}]}, {\"sentence\": \"\\\"I think this will be a moment when all of Europe says, 'Let us take a breath and let us figure out how do we maintain some of our national identities, how do we preserve the benefits of integration, and how do we deal with some of the frustrations that our own voters are feeling,'\\\" Mr. Obama said.\", \"from\": 965, \"to\": 1264, \"entities\": [], \"noun_phrases\": [{\"ngram\": \"a moment\", \"from\": 987, \"to\": 995}, {\"ngram\": \"Europe\", \"from\": 1008, \"to\": 1014}, {\"ngram\": \"us\", \"from\": 1026, \"to\": 1028}, {\"ngram\": \"a breath\", \"from\": 1034, \"to\": 1042}, {\"ngram\": \"us\", \"from\": 1051, \"to\": 1053}, {\"ngram\": \"our national identities\", \"from\": 1092, \"to\": 1115}, {\"ngram\": \"the benefits\", \"from\": 1136, \"to\": 1148}, {\"ngram\": \"integration\", \"from\": 1152, \"to\": 1163}, {\"ngram\": \"the frustrations\", \"from\": 1197, \"to\": 1213}, {\"ngram\": \"our own voters\", \"from\": 1219, \"to\": 1233}, {\"ngram\": \"Mr. Obama\", \"from\": 1249, \"to\": 1258}]}, {\"sentence\": \"\\\"I do not anticipate that there is going to be major cataclysmic changes as a consequence of this.\\\"\", \"from\": 1265, \"to\": 1364, \"entities\": [], \"noun_phrases\": [{\"ngram\": \"major cataclysmic changes\", \"from\": 1312, \"to\": 1337}, {\"ngram\": \"a consequence\", \"from\": 1341, \"to\": 1354}]}, {\"sentence\": \"He also pointed out there are other European countries that have strong relationships with the U.S. but are not members of the EU.\", \"from\": 1366, \"to\": 1496, \"entities\": [], \"noun_phrases\": [{\"ngram\": \"other European countries\", \"from\": 1396, \"to\": 1420}, {\"ngram\": \"strong relationships\", \"from\": 1431, \"to\": 1451}, {\"ngram\": \"the U.S.\", \"from\": 1457, \"to\": 1465}, {\"ngram\": \"members\", \"from\": 1478, \"to\": 1485}, {\"ngram\": \"the EU\", \"from\": 1489, \"to\": 1495}]}, {\"sentence\": \"\\\"Keep in mind that Norway is not a member of the European Union, but Norway is one of our closest allies,\\\" Mr. Obama said.\", \"from\": 1498, \"to\": 1620, \"entities\": [], \"noun_phrases\": [{\"ngram\": \"mind\", \"from\": 1507, \"to\": 1511}, {\"ngram\": \"Norway\", \"from\": 1517, \"to\": 1523}, {\"ngram\": \"a member\", \"from\": 1531, \"to\": 1539}, {\"ngram\": \"the European Union\", \"from\": 1543, \"to\": 1561}, {\"ngram\": \"Norway\", \"from\": 1567, \"to\": 1573}, {\"ngram\": \"our closest allies\", \"from\": 1584, \"to\": 1602}, {\"ngram\": \"Mr. Obama\", \"from\": 1605, \"to\": 1614}]}, {\"sentence\": \"\\\"They align themselves on almost every issue with Europe and us....[I]f over the course of what is going to be at least a two-year negotiation between England and Europe, Great Britain ends up being affiliated to Europe like Norway is, the average person is not going to notice a big change.\\\"\", \"from\": 1621, \"to\": 1913, \"entities\": [], \"noun_phrases\": [{\"ngram\": \"almost every issue\", \"from\": 1647, \"to\": 1665}, {\"ngram\": \"Europe\", \"from\": 1671, \"to\": 1677}, {\"ngram\": \"us\", \"from\": 1682, \"to\": 1684}, {\"ngram\": \"the course\", \"from\": 1698, \"to\": 1708}, {\"ngram\": \"at least a two-year negotiation\", \"from\": 1732, \"to\": 1763}, {\"ngram\": \"England\", \"from\": 1772, \"to\": 1779}, {\"ngram\": \"Europe\", \"from\": 1784, \"to\": 1790}, {\"ngram\": \"Great Britain\", \"from\": 1792, \"to\": 1805}, {\"ngram\": \"Europe\", \"from\": 1834, \"to\": 1840}, {\"ngram\": \"Norway\", \"from\": 1846, \"to\": 1852}, {\"ngram\": \"the average person\", \"from\":